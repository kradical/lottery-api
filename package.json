{
  "name": "lottery-api",
  "description": "A minimal template project for TypeScript interviews",
  "version": "1.0.0",
  "author": "kradical",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/neofinancial/interview-typescript.git"
  },
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "start": "ts-node index.ts",
    "lint": "eslint --ext .ts .",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "setupFiles": [
      "./app/setup-tests.ts"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/node-fetch": "^2.5.0",
    "@types/supertest": "^2.0.8",
    "eslint": "^5.16.0",
    "eslint-config-neo": "^0.3.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --quiet",
      "git add"
    ],
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
